import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();   //for tetscases
		while(T-->0){
		    int n = sc.nextInt();
		    float ans = (float)n*((float)13/2);
		    System.out.println((int)ans);
		}
	}
}
